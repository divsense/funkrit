export pureIO, runIO, IO
use 'ramda'

IO :: Eff(a) -> IO(a)
IO = run -> ({
    run,
    pure: pureIO,
    map: f -> IO $ f . run,
    chain: f -> IO $ runIO . f . run,
    ap: mv -> IO $ () -> runIO $ map(run(), mv)
})

pureIO :: a -> IO a
pureIO = a -> IO $ () -> a

runIO :: IO a -> a
runIO = call . prop('run')

